node {
      stage ('SCM Checkout') {
        git 'https://github.com/johnyc55/my-app-1'
      }
      stage('Compile-Package') {
        def mvnHome = tool name: 'mymvn-1', type: 'maven'
        sh "${mvnHome}/bin/mvn package"
      }
      stage ('Slack notifictcation') {
      slackSend baseUrl: 'https://hooks.slack.com/services/TEE680S9H/', channel: '# jenkins-notification', color: 'good', message: 'Welcome to Jenkins !!!', teamDomain: 'ION', tokenCredentialId: 'slack-demo'
      }
}
